= link_to request.env["HTTP_REFERER"] do
  %i.icon-arrow-left
  Takaisin
  |
  - if @material and @material.bakery
    = link_to :controller => "bakeries", :action => "show", :id => @material.bakery.id do
      %i.icon-home{title:"Koti"}
  - elsif @firm
    = link_to :controller => "firms", :action => "show", :id => @firm.resource.id do
      %i.icon-home{title:"Koti"}
  - elsif @substance and @substance.huslab
    = link_to :controller => "huslabs", :action => "show", :id => @substance.huslab.id do
      %i.icon-home{title:"Koti"}
  - elsif @eluate and @eluate.huslab
    = link_to :controller => "huslabs", :action => "show", :id => @eluate.huslab.id do
      %i.icon-home{title:"Koti"}
  - elsif @radiomedicine and @radiomedicine.huslab
    = link_to :controller => "huslabs", :action => "show", :id => @radiomedicine.huslab.id do
      %i.icon-home{title:"Koti"}
  - elsif @storagelocation and @storagelocation.huslab
    = link_to :controller => "huslabs", :action => "show", :id => @storagelocation.huslab.id do
      %i.icon-home{title:"Koti"}
  - else
    = link_to :controller => "huslabs", :action => "show", :id => current_user.primary_firm.resource.id do
      %i.icon-home{title:"Koti"}
